# AI Service Configuration
# Copy this file to .env and add your actual API keys

# Google Gemini Configuration (Recommended)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-1.5-flash

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4

# Alternative: Azure OpenAI Configuration
VITE_AZURE_OPENAI_ENDPOINT=your_azure_endpoint_here
VITE_AZURE_OPENAI_API_KEY=your_azure_api_key_here
VITE_AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name_here

# Alternative: Anthropic Claude Configuration
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AI Service Selection (gemini, openai, azure, anthropic)
VITE_AI_SERVICE=gemini
